<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fs.chatroom.mapper.GroupMsgMapper">


    <select id="getGroupMsgById" resultType="com.fs.chatroom.entity.pojo.ChatGroupMsg">
        select *
        from group_msg
        where group_id = #{groupId}
    </select>
</mapper>
